{
  "name": "construction-app",
  "version": "1.0.0",
  "dependencies": {
    "@supabase/ssr": "^0.8.0",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.2.0",
    "lucide-react": "^0.263.1",
    "next": "^16.1.1",
    "pg": "^8.11.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.0.0",
    "xlsx": "^0.18.5"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "inspect-schema": "node scripts/inspect-schema.js",
    "generate-types": "node scripts/generate-types.js",
    "validate-schema": "tsc --noEmit types/database.ts types/forms.ts",
    "check-varchar-compliance": "node scripts/check-varchar-compliance.js",
    "sync-schema": "npm run generate-types && npm run validate-schema && npm run check-varchar-compliance",
    "test:install": "npx playwright install",
    "test:api": "ALLOW_MAIN_DB_TESTING=true npx playwright test --project=api-tests",
    "test:e2e": "ALLOW_MAIN_DB_TESTING=true npx playwright test --project=e2e-critical",
    "test:all": "ALLOW_MAIN_DB_TESTING=true npx playwright test",
    "test:dev": "TEST_ENV=DEV npx playwright test",
    "test:main": "ALLOW_MAIN_DB_TESTING=true TEST_ENV=MAIN npx playwright test",
    "test:test": "TEST_ENV=TEST npx playwright test",
    "test:report": "npx playwright show-report tests/reports/html"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@supabase/supabase-js": "^2.90.0",
    "@types/uuid": "^10.0.0",
    "uuid": "^13.0.0"
  }
}
